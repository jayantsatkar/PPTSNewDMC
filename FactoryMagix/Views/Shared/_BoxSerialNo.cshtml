
@model IEnumerable<FactoryMagix.Models.BoxSerialNoReport>
@{

    Layout = null;
}
<div class="row">
    <div class="col-lg-12">
        <section class="panel-heading">
            <div class="form-group">

                <div class="row">

                    <div class="col-lg-8">
                        <label class="col-lg-2 control-labe6">Box SrNo</label>
                        <div class="col-lg-3">
                            <input type="text" id="txtPallet" tabindex="1" class="form-control" autofocus />
                        </div>


                        <div class="col-lg-2">
                            <input type="button" id="btnView" onclick="location.href='@Url.Action("BoxSerialWise", "PartailBoxSerialNo")?BoxSrNo=' + document.getElementById('txtPallet').value" tabindex="2" class="btn btn-default" title="View" value=" View  " />
                        </div>


                    </div>


                </div>

            </div>
        </section>

    </div>
</div>


<div class="row">
    <div class="col-lg-12">
        <section class="panel">

            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>

                            <th>
                                SrNo
                            </th>

                            <th>
                                Bosch PartNo
                            </th>
                            <th>
                                Part SrNo
                            </th>
                            <th>
                                Part BatchCode
                            </th>

                        </tr>
                    </thead>
                    <tbody>

                        @if (Model != null)
                        {
                            foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.SrNo)
                                    </td>
                                    <td>

                                        @Html.DisplayFor(modelItem => item.Boschpart_No)
                                    </td>
                                    <td>

                                        @Html.DisplayFor(modelItem => item.PartSerialNo)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PartBatchCode)
                                    </td>
                                </tr>
                            }
                        }

                    </tbody>
                </table>
            </div>

        </section>
    </div>
</div>



