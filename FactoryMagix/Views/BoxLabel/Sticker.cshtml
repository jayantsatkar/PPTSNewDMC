        // Added by 92293  get the list of part nos to bind to ddl
        function getPartNoList() {
            $.ajax({
                type: "POST",
                url: '@Url.Action("GetPartNos", "BoxLabel")',
                data: "{}",
                dataType: "json",
                traditional: true,
                success: function (data) {
